	    for(int i=10; i<900; i+=60) {
		createTu(i);
	    }
	}

    }
    
    public void createEnemy15() {
	if (count>100) { count=0;}
	count++;

	    for(int i=10; i<900; i+=80) {
		createGao(i);
	    }
	}
	
    }

    public void createEnemy16() {
	if (count>100) { count=0;}
	count++;

	if(count % 2 == 0) {
	    for(int i=0; i<950; i+=40) {
		createYu(i);
	    }
	}
	
    }

    public void createEnemy17() {
	if (count>100) { count=0;}
	count++;

	createBoss();

    }
