    public void createEnemy9() {
	if (count>100) { count=0;}
	count++;

	if (count % 70 == 0)
	    {   
		int x=(int) (Math.random()*(GameMain.GAME_WIDTH-180));
	        createGao(x);
	    }

	if (count % 20 == 0)
	    {   
		int x=(int) (Math.random()*(GameMain.GAME_WIDTH-180));
	        createTu(x);
	    }

	if (count % 50 == 0)
	    {   
		int x=(int) (Math.random()*(GameMain.GAME_WIDTH-180));
	        createFire(x);
	    }

    }

    public void createEnemy10() {
	if (count>100) { count=0;}
	count++;

	if (count % 1 == 0)
	    {   
		int x=(int) (Math.random()*(GameMain.GAME_WIDTH-180));
	        createYu(x);
	    }

	if (count % 11 == 0)
	    {   
		int x=(int) (Math.random()*(GameMain.GAME_WIDTH-180));
	        createTu(x);
	    }
    }

    public void createEnemy11() {
	if (count>100) { count=0;}
	count++;

	if(count % 100 == 0) {
	    for(int i=0; i<600; i+=100) {
		createNiao(i);
	    }
	}

    }

    public void createEnemy12() {
	if (count>100) { count=0;}
	count++;

	if(count % 50 == 0) {
	    for(int i=0; i<650; i+=60) {
		createNiu2(i);
	    }
	}

	if(frogCreated2 == false) {
	    createFrog(100);
	    createFrog(400);
	    createFrog(700);
	    frogCreated2 = true;
	}

    }

    public void createEnemy13() {
	if (count>100) { count=0;}
	count++;

	if(count % 88 == 0) {
	    for(int i=0; i<650; i+=100) {
		createFire(i);
	    }
	}


    }

    public void createEnemy14() {
	if (count>100) { count=0;}
	count++;

